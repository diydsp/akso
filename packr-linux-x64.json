{
    "platform": "linux64",
    "jdk": "external/jdks/linux_x64/OpenJDK11U-jdk_x64_linux_hotspot_11.0.8_10.tar.gz",
    "executable": "Akso",
    "classpath": [
        "axoloti/dist/Axoloti.jar"
    ],
    "removelibs": [
        "axoloti/dist/Axoloti.jar"
    ],
    "mainclass": "axoloti.Axoloti",
